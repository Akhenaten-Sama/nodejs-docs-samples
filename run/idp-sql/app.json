{
  "name": "idp-sql",
  "env": {
    "DB_PASSWORD": {
      "description": "postgreSQL password for root user"
    },
    "CLOUD_SQL_INSTANCE_NAME": {
      "description": "Cloud SQL instance name",
      "value": "idp-sql-instance"
    }
  },
  "hooks": {
    "precreate": {
      "commands": [
        "./setup.sh"
      ]
    },
    "postcreate": {
      "commands": [
        "./postcreate.sh"
      ]
    }
  }
}
